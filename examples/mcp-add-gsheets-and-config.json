{
  "tool": "mcp-playwright",
  "action": "exec",
  "args": {
    "steps": [
      { "click": "text=Add Node" },
      { "waitForSelector": "input[placeholder='Search nodes']" },
      { "type": "Google Sheets" },
      { "press": "Enter" },
      { "waitForSelector": "select[name='operation']" },
      { "select": { "selector": "select[name='operation']", "value": "getRows" } },
      { "waitForSelector": "input[name='spreadsheetId']" },
      { "typeInto": { "selector": "input[name='spreadsheetId']", "text": "1AbCxyz..." } },
      { "waitForSelector": "input[name='sheetName']" },
      { "typeInto": { "selector": "input[name='sheetName']", "text": "Data" } }
    ]
  }
}


